# Example macro for qsim

/qsim/filename qsim.out

/qsim/seed 50

/qsim/xmin -7.5 cm
/qsim/xmax  7.5 cm

/qsim/ymin  -0.75 cm
/qsim/ymax   0.75 cm

/qsim/z     -10.6 cm

/qsim/emin   850 MeV
/qsim/emax   850 MeV

/qsim/theta   45 deg
/qsim/phi      0 deg

/gun/particle e-
#/gun/particle mu-


/run/beamOn 1



