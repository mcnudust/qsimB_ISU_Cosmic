# Example macro for qsim

/qsim/filename /home/bulacarl/solid/data/qsimB_thickness_10mm_separation_54,5mm_events_1E3_Mirrors_thetaMin_126deg_thetaMax_126deg_phiMin_0deg_phiMax_0deg_Electrons_emin_850MeV_emax_850MeV_Source_xmin_0cm_xmax_0cm_ymin_0cm_ymax_0cm_zmin_1,5cm_zmax_1,5cm_QER7723Q_seed_122668.out

/qsim/seed 122668

/qsim/xmin 0.0 cm
/qsim/xmax 0.0 cm

/qsim/ymin 0.0 cm
/qsim/ymax 0.0 cm

/qsim/zmin 1.5 cm
/qsim/zmax 1.5 cm

/qsim/emin 850 MeV
/qsim/emax 850 MeV

/qsim/thetaMin 100.0 deg
/qsim/thetaMax 260.0 deg
/qsim/phiMin 0 deg
/qsim/phiMax 0 deg

/gun/particle e-
#/gun/particle e-

#/control/execute vis/Qt.mac

#/tracking/verbose 2

/run/beamOn 1000
