# Example macro for qsim

/qsim/filename /home/bulacarl/solid/data/qsim_thickness_10mm_separation_5mm_events_1E4_NoMirrors_AngleTheta_34,7_55,3_AnglePhi_-3,6_3,6_Muons_Source_z_-56cm_seed_10.out

/qsim/seed 10

/qsim/xmin -56.0 cm
/qsim/xmax -42.0 cm

/qsim/ymin  -1.75 cm
/qsim/ymax   1.75 cm

/qsim/z     -56.0 cm

/qsim/emin   850 MeV
/qsim/emax   850 MeV

/qsim/thetaMin   34.7 deg
/qsim/thetaMax   55.3 deg
/qsim/phiMin     -3.6 deg
/qsim/phiMax      3.6 deg

#/gun/particle e-
/gun/particle mu-

#/control/execute vis/Qt.mac

#/tracking/verbose 2

/run/beamOn 10000
