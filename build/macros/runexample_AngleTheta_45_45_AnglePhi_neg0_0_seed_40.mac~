# Example macro for qsim

/qsim/filename /home/bulacarl/solid/data/qsim_thickness_10mm_separation_5mm_events_1E5_NoMirrors_AngleTheta_45_45_AnglePhi_0_0_Muons_Source_xmin_-59_xmax_-39_ymin_-3,5_ymax_3,5_zmin_-65cm_zmax_-47cm_QER7723Q_seed_871124.out

/qsim/seed 871124

/qsim/xmin -59.0 cm
/qsim/xmax -39.0 cm

/qsim/ymin  -3.5 cm
/qsim/ymax   3.5 cm

/qsim/zmin -65.0 cm
/qsim/zmax -47.0 cm

/qsim/emin   850 MeV
/qsim/emax   850 MeV

/qsim/thetaMin   45 deg
/qsim/thetaMax   45 deg
/qsim/phiMin      0 deg
/qsim/phiMax      0 deg

#/gun/particle e-
/gun/particle mu-

#/control/execute vis/Qt.mac

#/tracking/verbose 2

/run/beamOn 100000
